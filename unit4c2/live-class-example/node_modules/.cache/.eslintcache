        setLoading(false)\r\n        })\r\n    }\r\n\r\n    const handleDelete = (id) => {\r\n        setLoading(true)\r\n        deleteTodo(id)\r\n        .then(res=>{\r\n            handleGetTodos()\r\n        })\r\n        .catch(err=>{\r\n            setLoading(false)\r\n        })\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            <div>\r\n                {loading && \"Loading!\"}\r\n            </div>\r\n            <AddTodo handleAdd={handleAdd} />\r\n            <div>\r\n                <button onClick={()=>setTitleSortBy(prev=>prev===\"ASC\"?\"DESC\":\"ASC\")} >\r\n                  TITLE:  {titleSortBy === \"ASC\" ? \"SHOW DESCENDING\": \"SHOW ASCENDING\"} \r\n                </button>\r\n            </div>\r\n            <h3>PENDING</h3>\r\n            {\r\n                todos.filter(item=>!item.status).map(item=>\r\n                    <TodoList \r\n                        key={item.id} \r\n                        title={item.title} \r\n                        status={item.status} \r\n                        id={item.id}\r\n                        handleToggle={handleToggle}\r\n                        handleDelete={handleDelete}\r\n                        />)\r\n            }\r\n            <h3>COMPLETED</h3>\r\n            {\r\n                todos.filter(item=>item.status).map(item=>\r\n                    <TodoList \r\n                        key={item.id} \r\n                        title={item.title} \r\n                        status={item.status} \r\n                        id={item.id}\r\n                        handleToggle={handleToggle}\r\n                        handleDelete={handleDelete}\r\n                        />)\r\n            }\r\n            <div>\r\n                <button onClick={()=>setPage(prev=>prev-1)} disabled={page===1} >PREV</button>\r\n                <button>{page}</button>\r\n                <button onClick={()=>setPage(prev=>prev+1)}>NEXT</button>\r\n            </div>\r\n            <Pagination total={10} current={page} onChange={(value)=>setPage(value)} />\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Todo","C:\\Users\\bittu\\Desktop\\Programming\\vivek\\web 19 React Unit 4\\live-class-example\\src\\Components\\Todos\\AddTodo.jsx",[],[],"C:\\Users\\bittu\\Desktop\\Programming\\vivek\\web 19 React Unit 4\\live-class-example\\src\\Components\\Todos\\TodoList.jsx",[],[],"C:\\Users\\bittu\\Desktop\\Programming\\vivek\\web 19 React Unit 4\\live-class-example\\src\\api\\todos.js",[],[],"C:\\Users\\bittu\\Desktop\\Programming\\vivek\\web 19 React Unit 4\\live-class-example\\src\\Components\\Todos\\Pagination.jsx",[],[],{"ruleId":"78","severity":1,"message":"79","line":1,"column":8,"nodeType":"80","messageId":"81","endLine":1,"endColumn":12},{"ruleId":"78","severity":1,"message":"79","line":1,"column":8,"nodeType":"80","messageId":"81","endLine":1,"endColumn":12},{"ruleId":"82","severity":1,"message":"83","line":17,"column":7,"nodeType":"84","endLine":17,"endColumn":25,"suggestions":"85"},"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'handleGetTodos'. Either include it or remove the dependency array.","ArrayExpression",["86"],{"desc":"87","fix":"88"},"Update the dependencies array to be: [titleSortBy, page, handleGetTodos]",{"range":"89","text":"90"},[529,547],"[titleSortBy, page, handleGetTodos]"]